{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A vertical box plot showing median, min, and max body mass of penguins.",
    "data": {"url": "https://raw.githubusercontent.com/arnav-bhalla/Data-Vis/main/NCD_world.csv"},
    "width": 400,
    "height": 400,
    "mark": {
      "type": "boxplot",
      "extent": "min-max"
    },
    "encoding": {
      "x": {"field": "Sex", "type": "nominal"},
      "y": {
        "field": "Mean BMI",
        "type": "quantitative",
        "scale": {"zero": false},
        "title" : "BMI"
      }
    }
  }